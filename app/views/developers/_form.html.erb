<%= form_for(@developer) do |f| %>

  <div class="field">
    <%= f.label :email, "Email" %><br>
    <%= f.email_field :email %>
  </div>
  <div class="field">
    <%= f.label :password, "Password" %><br>
    <%= f.password_field :password %>
  </div>
  <div class="field">
    <%= f.label :name, "Name"%><br>
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
